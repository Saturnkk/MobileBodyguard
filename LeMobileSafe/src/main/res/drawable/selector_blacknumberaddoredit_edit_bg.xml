<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--  state_focused和state_enabled两个条件同时满足的时候显示的突变-->
    <item 
          android:state_focused="true"
          android:state_enabled="true"
          android:drawable="@drawable/shape_blacknumberaddoredit_enable" /> <!-- state_focused ：获取焦点显示的图片      state_enabled：可用的时候显示图片-->
    <item android:drawable="@drawable/shape_blacknumberaddoredit_unenable" /> <!-- default ： 默认显示图片-->
</selector>